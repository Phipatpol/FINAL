<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>ล็อตเตอรี่ไทยแลนด์</title>
</head>
<body>
    <link href="showbyid.css" rel="stylesheet">
    <h1>Home</h1>
    
    <a href="http://localhost:5000/">หน้าแรก</a>
    <table id="table">
        <tr>
            <th>id</th>
            <th>Category</th>
            <th>Draw</th>
            <th>Prize</th>
            <th>Date</th>
        </tr>
        <% beers.forEach(function(item) { %>
        <tr>
            <td><%= item.id %></td>
            <td><%= item.Category %></td>
            <td><%= item.Draw %></td>
            <td><%= item.Prize %></td>
            <td><%= item.date %></td>
        </tr>
        <% }) %>
    </table>
</body>
</html>